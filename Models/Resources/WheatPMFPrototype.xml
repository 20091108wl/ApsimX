    <Plant>
	<Name>WheatPMFPrototype</Name>
	<CropType>Wheat</CropType>
	<Cultivar>
		<Name>hartog</Name>
		<Alias>hartog</Alias>
        <Command>[Phenology].Vernalisation.VernSens = 3</Command>
        <Command>[Phenology].Vernalisation.PhotopSens = 3</Command>
        <Command>[Phenology].FloralInitiationFloweringPhase.Target = 640</Command>
	</Cultivar>
	<Phenology>
		<Name>Phenology</Name>
		<AirTemperatureFunction>
            <Name>ThermalTime</Name>
            <XYPairs>
                <Name>XYPairs</Name>
                <X>
                    <double>0</double>
                    <double>26</double>
                    <double>34</double>
                </X>
                <Y>
                    <double>0</double>
                    <double>26</double>
                    <double>0</double>
                </Y>
            </XYPairs>
        </AirTemperatureFunction>
        <GerminatingPhase>
            <Name>SowingGermination</Name>
            <Start>Sowing</Start>
            <End>Germination</End>
        </GerminatingPhase>
        <EmergingPhase>
            <Name>GerminationEmergence</Name>
            <Start>Germination</Start>
            <End>Emergence</End>
            <ShootLag>40</ShootLag>
            <ShootRate>1.5</ShootRate>
            <VariableReference>
				<Name>ThermalTime</Name>
				<VariableName>[Phenology].ThermalTime.Value</VariableName>
			</VariableReference>
        </EmergingPhase>
        <GenericPhase>
            <Name>EmergenceEndOfJuvenilePhasePhase</Name>
            <Start>Emergence</Start>
            <End>EndOfJuvenile</End>
            <Constant>
                <Name>Target</Name>
                <Value>401</Value>
            </Constant>
            <VariableReference>
				<Name>ThermalTime</Name>
				<VariableName>[Phenology].ThermalTime.Value</VariableName>
			</VariableReference>
        </GenericPhase>
        <GenericPhase>
            <Name>EndOfJuvenileFloralInitiationPhase</Name>
            <Start>EndOfJuvenile</Start>
            <End>FloralInitiation</End>
            <MultiplyFunction>
                <Name>ThermalTime</Name>
                <VariableReference>
                    <Name>ThermalTime</Name>
                    <VariableName>[Phenology].ThermalTime.Value</VariableName>
                </VariableReference>                
                <PhaseLookup>
                    <Name>VernEff</Name>
                    <PhaseLookupValue>
                        <Name>VernEff0</Name>
                        <Start>Emergence</Start>
                        <End>FloralInitiation</End>
                        <VariableReference>
                            <Name>VernEff1</Name>
                            <VariableName>[Phenology].Vernalisation.VernEff</VariableName>
                        </VariableReference>
                    </PhaseLookupValue>
                    <PhaseLookupValue>
                        <Name>VernEff1</Name>
                        <Start>FloralInitiation</Start>
                        <End>Flowering</End>
                        <Constant>
                            <Name>VernEff1</Name>
                            <Value>1</Value>
                        </Constant>
                    </PhaseLookupValue>
                </PhaseLookup>
                <VariableReference>
                    <Name>PhotopEff</Name>
                    <VariableName>[Phenology].Vernalisation.PhotopEff</VariableName>
                </VariableReference>
            </MultiplyFunction>
            <Constant>
                <Name>Target</Name>
                <Value>400</Value>
            </Constant>
        </GenericPhase>
		<GenericPhase>
            <Name>FloralInitiationFloweringPhase</Name>
            <Start>FloralInitiation</Start>
            <End>Flowering</End>
            <MultiplyFunction>
                <Name>ThermalTime</Name>
                <VariableReference>
                    <Name>ThermalTime</Name>
                    <VariableName>[Phenology].ThermalTime.Value</VariableName>
                </VariableReference>                
                <PhaseLookup>
                    <Name>VernEff</Name>
                    <PhaseLookupValue>
                        <Name>VernEff0</Name>
                        <Start>Emergence</Start>
                        <End>FloralInitiation</End>
                        <VariableReference>
                            <Name>VernEff1</Name>
                            <VariableName>[Phenology].Vernalisation.VernEff</VariableName>
                        </VariableReference>
                    </PhaseLookupValue>
                    <PhaseLookupValue>
                        <Name>VernEff1</Name>
                        <Start>FloralInitiation</Start>
                        <End>Flowering</End>
                        <Constant>
                            <Name>VernEff1</Name>
                            <Value>1</Value>
                        </Constant>
                    </PhaseLookupValue>
                </PhaseLookup>
                <VariableReference>
                    <Name>PhotopEff</Name>
                    <VariableName>[Phenology].Vernalisation.PhotopEff</VariableName>
                </VariableReference>
            </MultiplyFunction>
            <Constant>
                <Name>Target</Name>
                <Value>555</Value>
            </Constant>
        </GenericPhase>
        <GenericPhase>
            <Name>FloweringStartGrainFillPHase</Name>
            <Start>Flowering</Start>
            <End>StartGrainFill</End>
            <Constant>
                <Name>Target</Name>
                <Value>120</Value>
            </Constant>
            <VariableReference>
				<Name>ThermalTime</Name>
				<VariableName>[Phenology].ThermalTime.Value</VariableName>
			</VariableReference>
        </GenericPhase>
        <GenericPhase>
            <Name>GrainFillingPhase</Name>
            <Start>StartGrainFill</Start>
            <End>EndGrainFill</End>
            <Constant>
                <Name>Target</Name>
                <Value>545</Value>
            </Constant>
            <VariableReference>
				<Name>ThermalTime</Name>
				<VariableName>[Phenology].ThermalTime.Value</VariableName>
			</VariableReference>
        </GenericPhase>
        <GenericPhase>
            <Name>MaturityPhase</Name>
            <Start>EndGrainFill</Start>
            <End>Maturity</End>
            <Constant>
                <Name>Target</Name>
                <Value>35</Value>
            </Constant>
            <VariableReference>
				<Name>ThermalTime</Name>
				<VariableName>[Phenology].ThermalTime.Value</VariableName>
			</VariableReference>
        </GenericPhase>
        <GenericPhase>
            <Name>HarvestRipePhase</Name>
            <Start>Maturity</Start>
            <End>HarvestRipe</End>
            <Constant>
                <Name>Target</Name>
                <Value>1</Value>
            </Constant>
            <VariableReference>
				<Name>ThermalTime</Name>
				<VariableName>[Phenology].ThermalTime.Value</VariableName>
			</VariableReference>
        </GenericPhase>
        <EndPhase>
            <Name>ReadyForHarvesting</Name>
            <Start>HarvestRipe</Start>
            <End>Unused</End>
            <VariableReference>
				<Name>ThermalTime</Name>
				<VariableName>[Phenology].ThermalTime.Value</VariableName>
			</VariableReference>
        </EndPhase>
		<PhotoperiodFunction>
			<Name>Photoperiod</Name>
			<Twilight>-6</Twilight>
		</PhotoperiodFunction>
		<VernalisationCW>
			<Name>Vernalisation</Name>
			<StartStageForEffects>Emergence</StartStageForEffects>
			<EndStageForEffects>Flowering</EndStageForEffects>
			<StartStageForCumulativeVD>Germination</StartStageForCumulativeVD>
			<EndStageForCumulativeVD>FloralInitiation</EndStageForCumulativeVD>
			<VernSens>3</VernSens>
			<PhotopSens>3</PhotopSens>
		</VernalisationCW>
	</Phenology>
</Plant>
