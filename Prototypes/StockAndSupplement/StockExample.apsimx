                  "Key": "targetDeficit",
                  "Value": "99"
                },
                {
                  "Key": "returndays",
                  "Value": "1"
                },
                {
                  "Key": "maximumAmount",
                  "Value": "30"
                },
                {
                  "Key": "depthPAWC",
                  "Value": "300"
                }
              ],
              "Name": "AutomaticIrrigation",
